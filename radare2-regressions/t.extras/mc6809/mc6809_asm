#!/bin/sh
for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done

unset R2_NOPLUGINS

# Tests only whole disassembly opcode
test_vector() {
NAME="${1}: [${2}]"
FILE=malloc://32
if [ "${5}" = "br" ]; then
	BROKEN=1
fi
CMDS='
e asm.arch='${1}'
wx '${3}'
pi 1
'
EXPECT="${4}
"
run_test
}

PLUGIN=mc6809


# plugin existence and bitness check
NAME="mc6809: checking bitness (asm.bits)"
FILE=malloc://32
CMDS='
e asm.arch=mc6809
e asm.bits
'
EXPECT='8
'
run_test

# Opcode tests
test_vector "${PLUGIN}" "ABX" 3A "abx"
