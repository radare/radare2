#!/bin/sh
for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done


test_gb() {
NAME="gb:${2}"
FILE=malloc://8
ARGS="-w -a gb"
CMDS='wx '${1}'
pi 1
'
EXPECT="${2}
"
run_test
}

