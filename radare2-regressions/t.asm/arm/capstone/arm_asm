#!/bin/sh
for a in . .. ../../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done

# notes
# -----
# r9 = sb
#
# gnu uses pre-UAL syntax:
# -ldrdne r2, [r3, ip]
#
# capstone uses the new one:
# +ldrdne r2, r3, [r3, ip]


test_vector() {
NAME="${1}: [${2}]"
FILE=malloc://32
if [ "${5}" = "br" ]; then
	BROKEN=1
	if [ -n "${TRAVIS}" ]; then
		unset BROKEN
		return
	fi
fi
CMDS='
e asm.arch='${1}'
e asm.bits=32
pad `pa '${4}'`
'
EXPECT="${4}
"
run_test
}

PLUGIN=arm

test_vector "${PLUGIN}" "ADDEQ r1, r1, r0" 00108100 "addeq r1, r1, r0"
test_vector "${PLUGIN}" "ADDEQ r1, r1, r2, lsl 2" 02118100 "addeq r1, r1, r2, lsl 2" "br"
test_vector "${PLUGIN}" "ADDNE r1, r1, ip" 0c108110 "addne r1, r1, ip"
test_vector "${PLUGIN}" "ADDNE r1, r1, r0, lsl 2" 00118110 "addne r1, r1, r0, lsl 2" "br"
test_vector "${PLUGIN}" "ANDEQ r0, r0, 1" 01000002 "andeq r0, r0, 1"
test_vector "${PLUGIN}" "ANDEQ r3, r5 0x80000000" 02310502 "andeq r3, r5, 0x80000000" "br"
test_vector "${PLUGIN}" "ANDNE r3, r3, r2" 02300310 "andne r3, r3, r2"
test_vector "${PLUGIN}" "ANDNE ip, ip, r7" 07c00c10 "andne ip, ip, r7"
test_vector "${PLUGIN}" "ASREQ r0, ipF, 0x1f" cc0fa001 "asreq r0, ip, 0x1f" "br"
test_vector "${PLUGIN}" "ASRNE r0, r4, 0x1f" c40fa011 "asrne r0, r4, 0x1f" "br"
test_vector "${PLUGIN}" "BEQ 8" 0000000a "beq 8"
test_vector "${PLUGIN}" "BICEQ r3, r3, 7" 0730c303 "biceq r3, r3, 7" "br"
test_vector "${PLUGIN}" "BLNE 0x1900" 3E06001B "blne 0x1900"
test_vector "${PLUGIN}" "BL 0x1900" 3e0600eb "bl 0x1900"
test_vector "${PLUGIN}" "B 0x1900" 3e0600ea "b 0x1900"
test_vector "${PLUGIN}" "BXEQ lr" 1eff2f01 "bxeq lr" "br"
test_vector "${PLUGIN}" "BXNE lr" 1eff2f11 "bxne lr" "br"
test_vector "${PLUGIN}" "CLZNE r5, sl" 1a5f6f11 "clzne r5, sl" "br"
test_vector "${PLUGIN}" "CMPEQ sl, r4" 40005a01 "cmpeq sl, r0, asr 32" "br"
test_vector "${PLUGIN}" "CMPNE r7, r6" 06005711 "cmpne r7, r6"
test_vector "${PLUGIN}" "EOREQ r2, r2, 1" 01202202 "eoreq r2, r2, 1"
test_vector "${PLUGIN}" "EORNE r0, r0, r3" 03002010 "eorne r0, r0, r3"
test_vector "${PLUGIN}" "EORNE r0, r0, r3, lsr 24" 230c2010 "eorne r0, r0, r3, lsr 24" "br"
test_vector "${PLUGIN}" "LDMDBEQ r2, {r0, r1}" 03001209 "ldmdbeq r2, {r0, r1}" "br"
test_vector "${PLUGIN}" "LDMEQ r3, {r0, r1, r2, r3}" 0f009308 "ldmeq r3, {r0, r1, r2, r3}" "br"
test_vector "${PLUGIN}" "LDMEQ r5, {r0, r1, r2}" 07009508 "ldmeq r5, {r0, r1, r2}" "br"
test_vector "${PLUGIN}" "LDMEQ r6, {r0, r1}" 03009608 "ldmeq r6, {r0, r1}" "br"
test_vector "${PLUGIN}" "LDRBEQ r5, [r7, -1]" 01505705 "ldrbeq r5, [r7, -1]" "br"
test_vector "${PLUGIN}" "LDRDNE r2, r3, [r3, ip]" dc208311 "ldrdne r2, r3, [r3, ip]" "br"
test_vector "${PLUGIN}" "LDREQ r0, [fp, -0xb4]" b4001b05 "ldreq r0, [fp, -0xb4]" "br"
test_vector "${PLUGIN}" "LDRHEQ r0, [r3, r0]" b0009301 "ldrheq r0, [r3, r0]" "br"
test_vector "${PLUGIN}" "LSLNE r1, r1, 2" 0111a011 "lslne r1, r1, 2" "br" "br"
test_vector "${PLUGIN}" "LSREQ r0, r0, 0x10" 2008a001 "lsreq r0, r0, 0x10" "br"
test_vector "${PLUGIN}" "LSRNE r0, r0, 9" a004a011 "lsrne r0, r0, 9" "br"
test_vector "${PLUGIN}" "MLAEQ r7, r5, r7, r0" 95072700 "mlaeq r7, r5, r7, r0" "br"
test_vector "${PLUGIN}" "MLANE r3, r1, r3, r2" 91232310 "mlane r3, r1, r3, r2" "br"
test_vector "${PLUGIN}" "MOVEQ r0, sl" 0a00a001 "moveq r0, sl"
test_vector "${PLUGIN}" "MOVNE r0, sb" 0900a011 "movne r0, sb" "br"
test_vector "${PLUGIN}" "MULNE r3, r3, r0" 93000310 "mulne r3, r3, r0" "br"
test_vector "${PLUGIN}" "MVNEQ r0, 0x15" 1500e003 "mvneq r0, 0x15"
test_vector "${PLUGIN}" "MVNEQ r0, 0x80000000" 0201e003 "mvneq r0, 0x80000000" "br"
test_vector "${PLUGIN}" "ORREQ r5, r5, r3" 03508501 "orreq r5, r5, r3"
test_vector "${PLUGIN}" "ORREQ r6, r6, r2, lsr 1" a2608601 "orreq r6, r6, r2, lsr 1" "br"
test_vector "${PLUGIN}" "ORREQ r3, r3, 0x80000000" 02318303 "orreq r3, r3, 0x80000000" "br"
test_vector "${PLUGIN}" "ORRNE r0, r0, r1, lsl ip" 110c8011 "orrne r0, r0, r1, lsl ip" "br"
test_vector "${PLUGIN}" "ORRNE r1, r1, r3" 03108111 "orrne r1, r1, r3"
test_vector "${PLUGIN}" "LDREQQ pc, [sp], 4" 04f09d04 "popeq {pc}" "br"
test_vector "${PLUGIN}" "POPEQ {r4, pc}" 1080bd08 "popeq {r4, pc}"
test_vector "${PLUGIN}" "POPEQ {r4, r5, pc}" 3080bd08 "popeq {r4, r5, pc}"
test_vector "${PLUGIN}" "POPEQ {r3, r4, r5, pc}" 3880bd08 "popeq {r3, r4, r5, pc}"
test_vector "${PLUGIN}" "POPEQ {r4, r5, r6, r7, pc}" f080bd08 "popeq {r4, r5, r6, r7, pc}"
test_vector "${PLUGIN}" "POPEQ {r3, r4, r5, r6, r7, pc}" f880bd08 "popeq {r3, r4, r5, r6, r7, pc}"
test_vector "${PLUGIN}" "POPEQ {r3, r4, r5, r6, r7, r8, sl, pc}" f885bd08 "popeq {r3, r4, r5, r6, r7, r8, sl, pc}"
test_vector "${PLUGIN}" "POPEQ {r3, r4, r5, r6, r7, r8, sb, sl, fp, pc}" f88fbd08 "popeq {r3, r4, r5, r6, r7, r8, sb, sl, fp, pc}" "br"
test_vector "${PLUGIN}" "REVEQ r3, r3" 333fbf06 "reveq r3, r3" "br"
test_vector "${PLUGIN}" "RSBEQ ip, r1, r7" 07c06100 "rsbeq ip, r1, r7"
test_vector "${PLUGIN}" "RSBNE r6, r0, r6" 06606010 "rsbne r6, r0, r6"
test_vector "${PLUGIN}" "STMEQ r0, {r2, r3}" 0c008008 "stmeq r0, {r2, r3}" "br"
test_vector "${PLUGIN}" "STMEQ ip, {r0, r1, r2}" 07008c08 "stmeq ip, {r0, r1, r2}" "br"
test_vector "${PLUGIN}" "STMEQ ip, {r0, r1, r2, r3}" 0f008c08 "stmeq ip, {r0, r1, r2, r3}" "br"
test_vector "${PLUGIN}" "STRBEQ ip, [r2, -1]" 01c04205 "strbeq ip, [r2, -1]" "br"
test_vector "${PLUGIN}" "STRBEQ r3, [r5]" 0030c505 "strbeq r3, [r5]" "br"
test_vector "${PLUGIN}" "STRDEQ r2, r3, [r4]" f020c401 "strdeq r2, r3, [r4]" "br"
test_vector "${PLUGIN}" "STRDEQ r4, r5, [r8, 0x50]" f045c801 "strdeq r4, r5, [r8, 0x50]" "br"
test_vector "${PLUGIN}" "STREQ r3, [r5, -4]" 04300505 "streq r3, [r5, -4]" "br"
test_vector "${PLUGIN}" "STREQ r1, [fp, -0x130]" 30110b05 "streq r1, [fp, -0x130]" "br"
test_vector "${PLUGIN}" "STRHNE r3, [fp, -0x32]" b2334b11 "strhne r3, [fp, -0x32]" "br"
test_vector "${PLUGIN}" "STRHEQ r3, [fp, -0x38]" b8334b01 "strheq r3, [fp, -0x38]" "br"
test_vector "${PLUGIN}" "STRHEQ r1, [r0]" b010c001 "strheq r1, [r0]" "br"
test_vector "${PLUGIN}" "STRHNE r1, [r2,r3]" b3108211 "strhne r1, [r2, r3]" "br"
test_vector "${PLUGIN}" "SUBEQ ip, r0, 0x20" 20c04002 "subeq ip, r0, 0x20"
test_vector "${PLUGIN}" "SUBNE r6, r3, r6, asr 1" c6604310 "subne r6, r3, r6, asr 1" "br"
test_vector "${PLUGIN}" "SUBNE r5, r5, r3" 03504510 "subne r5, r5, r3"
test_vector "${PLUGIN}" "SVCEQ 0" 0000000f "svceq 0"
test_vector "${PLUGIN}" "UMLALNE r4, r5, r3, r7" 9347a510 "umlalne r4, r5, r3, r7" "br"
test_vector "${PLUGIN}" "UXTBEQ r1, r0" 7010ef06 "uxtbeq r1, r0" "br"
test_vector "${PLUGIN}" "UXTHEQ r0, r0" 7000ff06 "uxtheq r0, r0" "br"
test_vector "${PLUGIN}" "VSTREQ d0, [r0]" 000b800d "vstreq d0, [r0]" "br"
test_vector "${PLUGIN}" "LSR r0, r1, r3" 3103a0e1 "lsr r0, r1, r3" 
test_vector "${PLUGIN}" "LSL r12, r8, r5" 1825a0e1 "lsl r2, r8, r5" 
test_vector "${PLUGIN}" "ASR r4, r5, r0" 5540a0e1 "asr r4, r5, r0" 
test_vector "${PLUGIN}" "ROR r7, r11, r1" 7671a0e1 "ror r7, r6, r1" 

NAME="${PLUGIN}: [BLNE 0x1900 @ 0xFF7A46EC]"
FILE=malloc://32
CMDS="!rasm2 -a ${PLUGIN} -b 32 -o 0xFF7A46EC -d 8374211b"
EXPECT="blne 0x1900
"
run_test
