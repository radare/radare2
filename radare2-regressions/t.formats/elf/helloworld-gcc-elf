#!/bin/sh
for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh && break ; done

NAME='x86 - init_array detection'
FILE=../../bins/elf/analysis/x86-helloworld-gcc
CMDS='aa;afl~0x080482e0[3]
'
EXPECT='loc.imp.__gmon_start__
'
run_test

