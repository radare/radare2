#!/bin/sh
for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done

# probably wrong
NAME='ELF: core sections'
FILE=../../bins/elf/analysis/core.1159
ARGS=''
CMDS="iS~?"
EXPECT='20
'
run_test

NAME='ELF: core sections with reg name'
FILE=../../bins/elf/analysis/core.1159
ARGS=''
BROKEN=1
CMDS="iS~?reg;iS"
EXPECT='6
'
run_test

