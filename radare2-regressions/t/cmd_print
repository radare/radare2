FILE=../bins/java/Hello.class
ARGS=-n
CMDS='
pm
'
EXPECT='0x00000000 1 Java CLASS
'
run_test

NAME='pm pe/a.exe'
FILE=../bins/pe/a.exe
ARGS=-n
CMDS='
pm
'
EXPECT='0x00000000 1 PE  for MS Windows (console) Intel 80386 32-bit
'
run_test

NAME='print raw lines'
FILE=../files/projects/resetc64
ARGS=
BROKEN=
CMDS='
sl 5
prl 1~[0]
e lines.abs=true
prl 1~[0]
'
EXPECT='+1
5
'
run_test
