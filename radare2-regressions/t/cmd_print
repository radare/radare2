run_test

NAME='pi -3 @ 3'
FILE=malloc://512
ARGS=
BROKEN=
CMDS='
e asm.arch=x86
e asm.bits=64
wx b8010000004839ca7f00
pi -3 @ 10
'
EXPECT='add byte [rax], al
cmp rdx, rcx
jg 0xa
'
run_test

NAME='pij -3 @ 3'
FILE=malloc://512
ARGS=
BROKEN=
CMDS='
e asm.arch=x86
e asm.bits=64
wx b8010000004839ca7f00
pij -3 @ 10~{}
'
EXPECT='[
  {
    "offset": 3,
    "val": 2,
    "esil": "al,rax,+=[1],$o,of,=,$s,sf,=,$z,zf,=,$c7,cf,=,$p,pf,=",
