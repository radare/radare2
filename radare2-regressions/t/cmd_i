[Imports]

0 imports
fs imports
[Entrypoints]

0 entrypoints
fs symbols
[Symbols]

0 symbols
fs symbols
[Sections]

0 sections
fs sections
"
# fd changes in each test, replace it with 42.
FILTER='sed "s/^fd       [0-9][0-9]*/fd       42/"'
run_test

NAME='i (all) (malloc) iI*'
BROKEN=
FILE=malloc://1024
BROKEN=
ARGS='-a x86' # see previous for caveat
CMDS='
i
i*
iI
iI*
'
EXPECT="file     malloc://1024
fd       42
size     0x400
iorw     true
blksz    0x0
mode     -rwx
block    0x100
format   any
e cfg.bigendian=false
e asm.bits=${BIT_SIZE}
e asm.dwarf=false
e asm.pcalign=0
havecode false
pic      false
canary   false
nx       false
crypto   false
va       false
bits     ${BIT_SIZE}
minopsz  1
maxopsz  16
pcalign  0
endian   little
stripped false
static   true
linenum  false
lsyms    false
relocs   false
binsz    1024
e cfg.bigendian=false
e asm.bits=${BIT_SIZE}
e asm.dwarf=false
e asm.pcalign=0
"
FILTER='sed "s/^fd       [0-9][0-9]*/fd       42/"'
run_test

NAME='i (file x86)'
FILE=../bins/elf/analysis/x86-helloworld-gcc
ARGS=
CMDS="i~!fd"
EXPECT='type     EXEC (Executable file)
