[ -e tests.sh ] && . ./tests.sh || . ../tests.sh

NAME='debruijn little endian'
FILE=-
ARGS=
BROKEN=
CMDS='
e cfg.bigendian=false
wopO 0x41574141
'
EXPECT='64
'
run_test

NAME='debruijn big endian'
FILE=-
ARGS=
BROKEN=
CMDS='
e cfg.bigendian=true
wopO 0x41574141
'
EXPECT='65
'
run_test

# -------------------------- #

NAME='wopD 0x10 @ 0'
FILE=-
ARGS=
BROKEN=
CMDS='wopD 0x10 @ 0
p8 0x10
'
EXPECT='41414142414143414144414145414146
'
run_test


