NAME=RISC-V ESIL for arithmetic word instructions
FILE=malloc://1024
EXPECT=<<EOF
0xfffffffe80000005
0xffffffff80000028
EOF
CMDS=<<EOF
e asm.arch=riscv
e asm.bits=64
wx 0x13 0x04 0x50 0x00 0x93 0x08 0x30 0x00 0x13 0x9f 0xf8 0x01 0x33 0x08 0xe4 0x41 0x3b 0x07 0x18 0x01 0xbb 0x03 0x07 0x03
aei
6aes
ar a6
ar t2
EOF
RUN