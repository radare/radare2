NAME=v850 random pseudo
FILE=bins/elf/ld-2.27.so
BROKEN=1
CMDS=<<EOF
s+128
e asm.arch=v850
e asm.parser=v850.pseudo
e asm.pseudo=true
b 64
pi~=
EOF
EXPECT=<<EOF
r8[0x1047] = (byte) r24
r0 = r0
r0 = r0
r0 = r0
r0 = r0
r0 == 0xa
r17 = 0x388 | r13
r0 = r0
r9 = r0
r11 = (0x8548 << 16) + r26
r2 == -0xc
r9 = r3
r9 = r3
r9 = r16
r9 = r8
r2 /= r19
r9 /= r19
r0 = r20
r9 = r0
EOF
RUN

